@model List<ResultsBlogDto>

<aside class="single_sidebar_widget popular_post_widget">
    <h3 class="widget_title" style="color: #2d2d2d;">Son Bloglar</h3>
   
    @foreach (var item in Model)
    {
        
    <div class="media post_item">
        <img style="width:50px;height:50px" src="@item.ImageURL" alt="post">
        <div class="media-body">
            <a href="blog_details.html">
                <h3 style="color: #2d2d2d;">@item.Title</h3>
            </a>
            <p>@item.BlogDate.ToString("d")</p>
        </div>
    </div>
    }
    
   
</aside>